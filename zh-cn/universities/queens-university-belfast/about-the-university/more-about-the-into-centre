        links[i].addEventListener('click', function () {
            analytics.track('Clicked Link', {
                target: $(this).attr("href"),
                title: document.title,
                label: $(this).attr("innerText"),
                link_text: $(this).attr("text"),
                baseURI: $(this).attr("baseURI")
            });
        });
    };
</script>
</body>
</html>