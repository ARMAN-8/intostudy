var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

/* eslint-disable camelcase */

var performanceTimingSchemaNames = ['chromeFirstPaint', // This value seems to be legacy
'connectEnd', 'connectStart', 'domainLookupEnd', 'domainLookupStart', 'domComplete', 'domContentLoadedEventEnd', 'domContentLoadedEventStart', 'domInteractive', 'domLoading', 'fetchStart', 'loadEventEnd', 'loadEventStart', 'msFirstPaint', // This value seems to be legacy
'navigationStart', 'proxyEnd', 'proxyStart', 'redirectEnd', 'redirectStart', 'requestEnd', // This value seems to be legacy
'requestStart', 'responseEnd', 'responseStart', 'secureConnectionStart', 'unloadEventEnd', 'unloadEventStart'];

function isValueInArray(val, array) {
  for (var i = 0; i < array.length; i++) {
    if (array[i] === val) {
      return true;
    }
  }
  return false;
}

// @author: Snowplow Analytics https://github.com/snowplow/
// Source: https://github.com/snowplow/snowplow-javascript-tracker/blob/814cbcfcb59fd8371cf1e16692db3a7b200c15b6/src/js/tracker.js#L1035
function getSnowplowPerformanceTimingContext(windowAlias) {
  var performance = windowAlias.performance || windowAlias.mozPerformance || windowAlias.msPerformance || windowAlias.webkitPerformance;
  if (performance) {
    // On Safari, the fields we are interested in are on the prototype chain of
    // performance.timing so we cannot copy them using lodash.clone
    var performanceTiming = {};
    for (var field in performance.timing) {
      if (isValueInArray(field, performanceTimingSchemaNames) && performance.timing[field] !== null) {
        performanceTiming[field] = performance.timing[field];
      }
    }

    // Old Chrome versions add an unwanted requestEnd field
    delete performanceTiming.requestEnd;

    return {
      schema: 'iglu:org.w3/PerformanceTiming/jsonschema/1-0-0',
      data: performanceTiming
    };
  }

  return null;
}

var intervalSeconds = 1;
var minimumVisitLengthSeconds = 2;

var initialized = false;

window.__SnowplowTrackerClass = function () {
  function SnowplowTracker() {
    var snowplow = arguments.length <= 0 || arguments[0] === undefined ? window.snowplow : arguments[0];
    var trackingData = arguments[1];
    var globalConfig = arguments[2];

    _classCallCheck(this, SnowplowTracker);

    if (!snowplow) {
      throw Error('Snowplow does not exist');
    }

    var _globalConfig$snowplo = globalConfig.snowplowSchemas;
    var conversionSchema = _globalConfig$snowplo.conversion;
    var landingPageSchema = _globalConfig$snowplo.landingPage;
    var snowplowEventsCollectorHost = globalConfig.snowplowEventsCollectorHost;


    this.conversionSchema = conversionSchema;
    this.landingPageSchema = landingPageSchema;
    this.eventsCollectorHost = snowplowEventsCollectorHost;
    this.snowplow = snowplow;

    if (!initialized) {
      this.initialize();
      this.setContext(trackingData);
    }
  }

  _createClass(SnowplowTracker, [{
    key: 'setContext',
    value: function setContext(trackingData) {
      var subaccount_id = trackingData.customerId;
      var lp_id = trackingData.pageId;
      var lp_version = trackingData.publishedVersion;
      var lp_variation_id = trackingData.variationId;


      this.context = {
        schema: this.landingPageSchema,
        data: {
          subaccount_id: subaccount_id,
          lp_id: lp_id,
          lp_version: lp_version,
          lp_variation_id: lp_variation_id
        }
      };
      this.snowplow('addGlobalContexts', [this.context]);
    }
  }, {
    key: 'initialize',
    value: function initialize() {
      this.snowplow('newTracker', 'cf', this.eventsCollectorHost, {
        appId: 'instapage',
        contexts: {
          webPage: true
        },
        platform: 'web',
        postPath: '/t/two'
      });

      this.enableActivityTracking(minimumVisitLengthSeconds, intervalSeconds);

      initialized = true;
    }
  }, {
    key: 'enableActivityTracking',
    value: function enableActivityTracking(minimumVisitLength, heartBeat) {
      this.snowplow('enableActivityTracking', minimumVisitLength, heartBeat);
    }
  }, {
    key: 'trackVisit',
    value: function trackVisit() {
      var performanceContext = getSnowplowPerformanceTimingContext(window);
      this.snowplow('trackPageView', null, [performanceContext]);
    }
  }, {
    key: 'trackLinkConversion',
    value: function trackLinkConversion(_ref) {
      var href = _ref.href;

      this.snowplow('trackUnstructEvent', {
        schema: this.conversionSchema,
        data: {
          targetUrl: href,
          type: 'link'
        }
      });
    }
  }, {
    key: 'trackLeadConversion',
    value: function trackLeadConversion() {
      this.snowplow('trackUnstructEvent', {
        schema: this.conversionSchema,
        data: {
          targetUrl: null,
          type: 'form'
        }
      });
    }
  }, {
    key: 'trackExternalConversion',
    value: function trackExternalConversion() {
      this.snowplow('trackUnstructEvent', {
        schema: this.conversionSchema,
        data: {
          targetUrl: null,
          type: 'external'
        }
      });
    }
  }]);

  return SnowplowTracker;
}();
</script>

<script type="text/javascript" class="instapage-noscrap">
	window.__conversions_settings = {"external":null,"forms":null,"links_enabled":null,"changed_by_user":null,"links":false};

	function iCopyAnalyticsEvent( name, properties )
	{
		var params_copy = window.__analytics_called_parameters.slice();
		var data = {};
		properties = properties || {};

		for( i in params_copy[ 2 ] )
		{
			if( params_copy[ 2 ].hasOwnProperty( i ) )
			{
				data[ i ] = params_copy[ 2 ][ i ];
			}
		}

		for( i in properties )
		{
			if( properties.hasOwnProperty( i ) )
			{
				data[ i ] = properties[ i ];
			}
		}

		if( name === 'conversion' && window.__unique )
		{
			if( window.__unique.isVisited() )
			{
				data[ 'responsive_mode' ] = window.__unique.isResponsiveMode() ? 'mobile' : null;
			}
		}


		params_copy.splice( 1, 2, name, data );
		return params_copy;
	}

	function removeEventParameter( name )
	{
		if( window.__analytics_called_parameters ) {
			var parameters = window.__analytics_called_parameters[2] ? window.__analytics_called_parameters[2] : {};

			if( parameters[name] )
			{
				delete parameters[name];
				window.__analytics_called_parameters[2] = parameters;
			}
		}
	}

	function iEncodePixelUrl( id, event, data )
	{
		var endpoint = "anthill.instapage.com" + '/api/v3/projects/' + id + '/events/' + event;
		var base64 = window.base64_encode( JSON.stringify( data ) );
		var base64_url_safe = base64.replace(/\+/g, '-').replace(/\//g, '_');
