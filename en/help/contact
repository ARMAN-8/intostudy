                <script type="text/javascript">
                    $('.expander-control').click(function (e) {
                        e.preventDefault();

                        $('.l-contact-expander-section').removeClass('is-open');
                        $($(e.target).attr('data-target')).addClass('is-open');

                        if ($(window).width() < 1030) {
                            $('body').animate({
                                scrollTop: $('#details-expander').offset().top
                            }, 500);
                        }
                    });

                    $('.contact-expander-close-button').click(function (e) {
                        e.preventDefault();
                        $('.l-contact-expander-section').removeClass('is-open');
                    });

                    $('#message').bind('input propertychange', function (e) {
                        var words = $('#message').val().split(/\s+/);
                        countdown = 500;

                        if (words[words.length - 1] === "") {
                            words.pop();
                        }

                        countdown -= words.length;

                        if (countdown <= 0) {
                            var fullText = words.slice(0, 500).join(" ");

                            $('#message').val(fullText);

                            countdown = 0;
                        }

                        $('#message-word-count').text(countdown);
                    });
                </script>
