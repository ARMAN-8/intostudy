<script>
    function initComponents() {
        var typeAhead = document.querySelector('into-typeahead');
        typeAhead.searchApiBaseUrl = 'https://api.intoglobal.com/search' + '/api/' + 'v1.0';


            typeAhead.headingText = 'What do you want to study?';
            typeAhead.placeholderText = 'eg. Computer Science';
            typeAhead.ctaText = 'FIND MY COURSE';
                        typeAhead.addEventListener('selectedSearchEvent', (e) => selectedItemEvent(e));
        }
    function selectedItemEvent(e) {
        var csUrl = 'https://search.intostudy.com/ar/search?searchTerm=' + e.detail.term;
        if (e.detail.courseType) {
            csUrl += "&courseType=" + e.detail.courseType.toLowerCase();
        }
        if (e.detail.country) {
            csUrl += "&country=" + e.detail.country;
        }
        csUrl += "&sortField=Relevancy&sortOrder=desc";

        window.location = encodeURI(csUrl);
    }
    $(document).ready(function () {
        initComponents();
    })
</script>
