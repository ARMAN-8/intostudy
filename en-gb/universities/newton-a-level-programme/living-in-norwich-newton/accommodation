        <div id="banner" class="hero-outer">
                <div class="l-hero eng-lang no-filter" style="background-image:url(https://ctfimages.intoglobal.com/xw2h6mjophtz/9VqEwaXvhYQmeMAyWOGKS/95b8251d410023a6a4759bb216fd83d6/Generic_Instapage_headersGeneric_INTO_student_photos.jpg?w=1400&h=788&fit=fill&q=75);height: 300px;"></div>
