      <section class="l-content-container outer-central-img">
        <div class="central-img">          
          <div class="DNNModuleContent ModDNNHTMLC">
            <div>
              <img src="/images/ImagesINTO/MAR/Landing/INTO-MAR-intro-students-stand-infront-Huntington-sign.jpg" alt="Life in Huntington">
              <div class="caption_holder">
                <p>
                  Making Huntington your new home
                </p>
              </div>
